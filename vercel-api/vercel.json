{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/pages/:pageId",
      "destination": "/src/api/pages.ts"
    },
    {
      "source": "/api/search",
      "destination": "/src/api/search.ts"
    }
  ]
}